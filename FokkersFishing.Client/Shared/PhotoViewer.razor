@inject HttpClient HttpClient

<MatDialog @bind-IsOpen="@dialogIsOpen" >
    <MatDialogTitle>#: @catchMade.CatchNumber (@catchMade.Fish - @catchMade.Length cm). <br />@catchMade.CatchDate.ToLocalTime()</MatDialogTitle>
    <MatDialogContent>
            <img src="@url" width="300px" />
        @if (isAdmin)
        {
                <a href="@url">Download measure photo</a>
        }
    </MatDialogContent>
    <MatDialogActions>
        <MatButton OnClick="@OnClose" Class="btn btn-secondary">Close</MatButton>
    </MatDialogActions>
</MatDialog>

@code {
    [Parameter] public string url { get; set; }
    [Parameter] public Catch catchMade { get; set; }
    [Parameter] public EventCallback OnClose { get; set; }
    [Parameter] public bool dialogIsOpen { get; set; }
    [Parameter] public bool isAdmin { get; set; }

}
